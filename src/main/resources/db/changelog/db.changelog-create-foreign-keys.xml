<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1" author="mikolajKubinski">
        <addForeignKeyConstraint
                baseTableName="borrow"
                baseColumnNames="idBook"
                referencedTableName="book"
                referencedColumnNames="id"  constraintName="bookBorrowKey"/>
        <addForeignKeyConstraint
                baseTableName="borrow"
                baseColumnNames="idReader"
                referencedTableName="reader"
                referencedColumnNames="id"  constraintName="readerBorrowKey"/>
    </changeSet>
</databaseChangeLog>